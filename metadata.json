{
  "icon": "icon.png",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "backend.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "usbmuxd",
      "src": "index.html",
      "root": "ui",
      "backend": {
        "socket": "backend.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          { "path": "/host/darwin/mobile-relay" },
          { "path": "/scripts/launchctl-helper.sh" }
        ],
        "linux": [
          { "path": "/host/linux/mobile-relay" },
          { "path": "/scripts/launchctl-helper.sh" }
        ]
      }
    ]
  }
}
